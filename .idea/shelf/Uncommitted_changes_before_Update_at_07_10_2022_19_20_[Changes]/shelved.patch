Box<>();\r\n\r\n    @FXML\r\n    ChoiceBox<String> grade = new ChoiceBox<>();\r\n    String[] prf = {\"Enseignant\",\"Doctorant\",\"Docteur\"};\r\n    String[] grd = {\"PES\",\"PH\",\"PA\"};\r\n\r\n\r\n    @FXML\r\n    public void initialize(URL url, ResourceBundle rb) {\r\n        profile.getItems().addAll(prf);\r\n        grade.getItems().addAll(grd);\r\n\r\n\r\n    }\r\n\r\n\r\n\r\n\r\n\r\n}\r\n
